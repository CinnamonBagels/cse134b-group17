{
  "name": "CSE134B Chrome Extension",

  "manifest_version": 2,

  "version": "1.0",

  "description": "MemeMaster",

  "browser_action": {
  	"default_title": "Gaben is our one true God",
    "default_icon": "img/gaben.png",
    "default_popup": "popup.html"
  },
  
  "content_security_policy": "script-src 'self' https://cdn.firebase.com https://*.firebaseio.com; object-src 'self'",


  "permissions": [
    "<all_urls>",
    "contextMenus"
    ],
  "icons": {
    
  },
  "background": {
    "persistent": true,
    "page": "popup.html"
    
  }
  }